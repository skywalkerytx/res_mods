<mmps>
  <config>
    <enableEffArmor>true</enableEffArmor>    <!-- Displays effective armor calculation. ** only when armor values are present in "vehiclesData" section. -->
    <enableRulerWhenTD>true</enableRulerWhenTD>    <!-- Displays GunArc Critical Angle Ruler for TD's. -->
    <enableRulerWhenSPG>true</enableRulerWhenSPG>    <!-- Displays GunArc Critical Angle Ruler for SPG's. -->
    <minimapEx>true</minimapEx>    <!-- Enables MiniMap Enhancement. -->
    <damagePanelEx>true</damagePanelEx>  <!-- Enables Damage Panel Enhancement. (Displays time to repair). -->
    <bigMapKey>666</bigMapKey>    <!-- Key to zoom MiniMap, default 20(CapsLock), can be changed to 17(Ctrl). -->
    <bigMapSize>600</bigMapSize>   <!-- Size of MiniMap zoom in pixels. -->
    <ordinateWhenTank>0.90</ordinateWhenTank>    <!-- Vertical placement of Effective armor calculation ( In percentage of lower half vertical screen resolution ). -->
    <ordinateWhenTD>0.87</ordinateWhenTD>    <!-- Vertical placement of TD GunArc Ruler ( where 0 is screen center and 1 its bottom ). -->
    <ordinateWhenSPG>0</ordinateWhenSPG>    <!-- Vertical placement of SPG GunArc Ruler. -->
    <timeOrange>180</timeOrange> <!-- Time after with (in seconds) battle timer flashes orange alert. -->
    <offsetX>-370</offsetX> <!-- Battle Timer "X" position in pixels (Default one in upper right corner). Relative to upper right corner of screen. Default is -370 -->
    <offsetY>15</offsetY> <!-- Battle Timer "Y" position in pixels (Default one in upper right corner). Relative to upper right corner of screen. Default is 15 -->
    <m2pix>8.2</m2pix>    <!-- Distance to Pixels ratio, Default is 1m = 8.2px. -->
    <xSpacing>0</xSpacing> <!-- GunArc Ruler degre dash'es spacing, default is 0 adaptive. Recommended setting for fixed spacing is 16 or 32. -->
    <enableClock>true</enableClock> <!-- Displays in-game realtime clock. -->
    <circle>50</circle>    <!-- View Range Circle Settings. View Range circle size, any meter range as value of viewrange. -->
    <circle>445</circle>	<!-- You'll need at least 2 <circle> entries for this feature to work properly -->
  </config>
